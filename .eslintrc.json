{
  "extends": "@rocketseat/eslint-config/node",
  "rules": {
    "no-console": "warn",
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "semi": true,
        "endOfLine": "auto"
      }
    ]
  }
}
